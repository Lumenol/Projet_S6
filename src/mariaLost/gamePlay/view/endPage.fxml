<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.VBox?>

<BorderPane prefHeight="600.0" prefWidth="600.0" styleClass="pane" stylesheets="@styleSheet.css" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="mariaLost.gamePlay.view.EndPageController">
   <center>
      <GridPane fx:id="rootPane" hgap="20.0" minHeight="-Infinity" minWidth="-Infinity" prefHeight="144.0" prefWidth="490.0">
          <columnConstraints>
              <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" percentWidth="20.0" />
              <ColumnConstraints hgrow="SOMETIMES" maxWidth="150.0" minWidth="10.0" prefWidth="76.0" />
              <ColumnConstraints hgrow="SOMETIMES" maxWidth="195.0" minWidth="0.0" prefWidth="0.0" />
          </columnConstraints>
          <rowConstraints>
            <RowConstraints />
            <RowConstraints />
            <RowConstraints maxHeight="202.0" minHeight="10.0" prefHeight="100.0" vgrow="SOMETIMES" />
            <RowConstraints maxHeight="30.0" minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
              <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          </rowConstraints>
          <padding>
              <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
          </padding>
         <children>
            <Label fx:id="nameLabel" prefHeight="82.0" prefWidth="109.0" text="Player" GridPane.columnIndex="1" GridPane.rowIndex="2" />
            <Label prefHeight="82.0" prefWidth="109.0" text="Level" GridPane.columnIndex="1" GridPane.rowIndex="5" />
            <Label prefHeight="82.0" prefWidth="109.0" text="Score" GridPane.columnIndex="1" GridPane.rowIndex="6" />
            <Label fx:id="levelLabel" prefHeight="82.0" prefWidth="109.0" text="Level" GridPane.columnIndex="2" GridPane.rowIndex="5" />
            <Label fx:id="scoreLabel" prefHeight="82.0" prefWidth="109.0" text="Score" GridPane.columnIndex="2" GridPane.rowIndex="6" />
            <ImageView fx:id="avatarImageView" fitHeight="153.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true" GridPane.columnIndex="2" GridPane.rowIndex="2" />
            <Label text="Life" GridPane.columnIndex="1" GridPane.rowIndex="4" />
            <Label text="Money" GridPane.columnIndex="1" GridPane.rowIndex="3" />
            <Label fx:id="lifeLabel" text="Label" GridPane.columnIndex="2" GridPane.rowIndex="4" />
            <Label fx:id="moneyLabel" text="Label" GridPane.columnIndex="2" GridPane.rowIndex="3" />
         </children>
      </GridPane>
   </center>
   <top>
       <Label fx:id="text" alignment="CENTER" contentDisplay="CENTER" prefHeight="100.0" prefWidth="520.0" text="GAME OVER" textAlignment="CENTER" BorderPane.alignment="CENTER">
         <opaqueInsets>
            <Insets />
         </opaqueInsets></Label>
   </top>
   <bottom>
      <VBox alignment="CENTER" prefHeight="121.0" prefWidth="600.0" BorderPane.alignment="CENTER">
         <children>
            <Button defaultButton="true" mnemonicParsing="true" onAction="#handlePlay" prefHeight="52.0" prefWidth="462.0" text="Play" />
          <Button mnemonicParsing="false" onAction="#handleMenu" prefHeight="27.0" prefWidth="176.0" text="Menu">
               <VBox.margin>
                  <Insets top="20.0" />
               </VBox.margin>
            </Button>
         </children>
      </VBox>
   </bottom>
</BorderPane>
