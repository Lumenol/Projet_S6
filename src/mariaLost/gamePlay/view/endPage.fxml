<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.*?>
<BorderPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="443.0" prefWidth="520.0"
            xmlns="http://javafx.com/javafx/8.0.111" fx:controller="mariaLost.gamePlay.view.EndPageController">
    <center>
        <GridPane fx:id="rootPane" hgap="20.0" minHeight="-Infinity" minWidth="-Infinity" prefHeight="339.0"
                  prefWidth="520.0">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" percentWidth="20.0"/>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="150.0" minWidth="10.0" prefWidth="76.0"/>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="195.0" minWidth="0.0" prefWidth="0.0"/>
            </columnConstraints>
            <rowConstraints>
                <RowConstraints/>
                <RowConstraints maxHeight="202.0" minHeight="10.0" prefHeight="186.0" vgrow="SOMETIMES"/>
                <RowConstraints maxHeight="86.0" minHeight="10.0" prefHeight="39.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            </rowConstraints>
            <padding>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
            </padding>
            <children>
                <Label fx:id="nameLabel" prefHeight="82.0" prefWidth="109.0" text="Player" GridPane.columnIndex="1"
                       GridPane.rowIndex="1"/>
                <Label prefHeight="82.0" prefWidth="109.0" text="Level" GridPane.columnIndex="1" GridPane.rowIndex="2"/>
                <Label prefHeight="82.0" prefWidth="109.0" text="Score" GridPane.columnIndex="1" GridPane.rowIndex="3"/>
                <Label fx:id="levelLabel" prefHeight="82.0" prefWidth="109.0" text="Level" GridPane.columnIndex="2"
                       GridPane.rowIndex="2"/>
                <Label fx:id="scoreLabel" prefHeight="82.0" prefWidth="109.0" text="Level" GridPane.columnIndex="2"
                       GridPane.rowIndex="3"/>
                <ImageView fx:id="avatarImageView" fitHeight="200.0" fitWidth="200.0" pickOnBounds="true"
                           preserveRatio="true" GridPane.columnIndex="2" GridPane.rowIndex="1"/>
            </children>
        </GridPane>
    </center>
    <top>
        <Label fx:id="text" prefHeight="100.0" prefWidth="520.0" text="GAME OVER" textAlignment="CENTER"
               BorderPane.alignment="CENTER">
            <opaqueInsets>
                <Insets/>
            </opaqueInsets>
        </Label>
    </top>
    <bottom>
        <ToolBar prefHeight="40.0" prefWidth="200.0" BorderPane.alignment="CENTER">
            <items>
                <Button mnemonicParsing="false" onAction="#handleMenu" text="Menu"/>
                <Button mnemonicParsing="true" onAction="#handlePlay" text="Play"/>
            </items>
        </ToolBar>
    </bottom>
</BorderPane>
