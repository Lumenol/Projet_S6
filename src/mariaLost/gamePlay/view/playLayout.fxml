<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<ButtonBar xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.111"
           fx:controller="mariaLost.gamePlay.controller.MenuBarController">
    <buttons>
        <Label fx:id="nameLabel" prefHeight="17.0" prefWidth="103.0" text="Player's name"/>
        <Label minWidth="59.0" prefHeight="17.0" text="Level"/>
        <Label fx:id="levelLabel" text="0"/>
        <Label text="Score"/>
        <Label fx:id="scoreLabel" alignment="CENTER_LEFT" nodeOrientation="LEFT_TO_RIGHT" text="0"/>
        <Button maxWidth="-Infinity" mnemonicParsing="false" onAction="#handleMenu" prefHeight="27.0" text="Menu"/>
    </buttons>
</ButtonBar>
